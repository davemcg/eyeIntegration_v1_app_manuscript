  keywords = {Choroid,Gene expression,Macular Degeneration,RNA-Seq,RPE,Retina,Retinitis pigmentosa,Transcriptome},
  pages = {93-106},
  file = {/Users/davidmcgaughey/Zotero/storage/X8PHFXXG/Whitmore et al. - 2014 - Transcriptomic analysis across nasal, temporal, an.pdf;/Users/davidmcgaughey/Zotero/storage/SMFE2V3Q/S0014483514002978.html}
