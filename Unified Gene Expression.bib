  journal = {Ophthalmology},
  author = {Ellingford, Jamie M. and Barton, Stephanie and Bhaskar, Sanjeev and Williams, Simon G. and Sergouniotis, Panagiotis I. and O'Sullivan, James and Lamb, Janine A. and Perveen, Rahat and Hall, Georgina and Newman, William G. and Bishop, Paul N. and Roberts, Stephen A. and Leach, Rick and Tearle, Rick and Bayliss, Stuart and Ramsden, Simon C. and Nemeth, Andrea H. and Black, Graeme C. M.},
  month = may,
  year = {2016},
  pages = {1143-1150},
  file = {/Users/davidmcgaughey/Zotero/storage/X4KWPVNW/Ellingford et al. - 2016 - Whole Genome Sequencing Increases Molecular Diagno.pdf;/Users/davidmcgaughey/Zotero/storage/AM5W3XWI/S0161642016000300.html}
}

